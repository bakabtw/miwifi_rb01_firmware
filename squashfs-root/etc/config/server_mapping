config location dftlocation
        option location 'RU'

config server 'server_CN'
	option API api.miwifi.com
	option S s.miwifi.com
	option APP app.miwifi.com
	option STUN stun.miwifi.com
	option BROKER api.miwifi.com

config server 'server_IN'
